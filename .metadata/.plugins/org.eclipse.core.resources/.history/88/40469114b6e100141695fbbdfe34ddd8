#include "Platform.h"
#include "Application.h"
#include "LED.h"
#include "WAIT1.h"
#include "Event.h"
#include "RTOS.h"

void APP_HandleEvents(EVENT_Handle event)
{
	switch(event)
	{
	case EVENT_Start:
		LED1_ON;
		WAIT1_Waitms(200)
		LED1_OFF;
	}
}


